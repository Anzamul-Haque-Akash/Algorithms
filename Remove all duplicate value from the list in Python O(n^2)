numbers = []
n = int(input("Enter the lenth of the list : "))

for i in range(n):
    data = int(input('Input : '))
    numbers.append(data)

while True:
    con = True
    for i in range(len(numbers)):
        count1 = int(numbers.count(numbers[i]))
        if count1 > 1:
            con = False
            count1 -= 1
            for j in range(count1):
                numbers.remove(numbers[i])
            break
    if con == True:
        break;

print(numbers)

